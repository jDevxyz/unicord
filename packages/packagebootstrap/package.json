{
  "name": "@unicord/packagebootstrap",
  "version": "1.0.0",
  "description": "Bootstrapping package, for developing purpose",
  "author": "BillyAddlers <finnsonalca123@gmail.com>",
  "homepage": "https://j-dev.xyz/projects/unicord",
  "license": "AGPL-3.0",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
